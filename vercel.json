{
    "version": 2,
    "builds": [
        {
            "src": "backend/src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "backend/src/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}